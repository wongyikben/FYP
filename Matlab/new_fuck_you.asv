function []=new_fuck_you(input)

result = zeros(1,length(input));
for i=1:length(input)
   x = input(1:3,i); 
   offset = mean(x);
   x = x-offset;
   sine = (x(1)-x(2)-x(3))/2;
   cose = (x(2)-x(3))/(2*sin(2*pi/3));
   result(i)=atan2(sine,cose);
  
end

result = int32(result*72/(2*pi));



plot(result)